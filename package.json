{
  "name": "alexandrahedin-astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lighthouse:local": "npm run lighthouse:local:en && npm run lighthouse:local:sv",
    "lighthouse:local:en": "lighthouse http://localhost:4321 --only-categories=performance,accessibility,best-practices,seo --output=html --output-path=./lighthouse-reports/local-en.html --chrome-flags=\"--headless\" --quiet",
    "lighthouse:local:sv": "lighthouse http://localhost:4321/sv --only-categories=performance,accessibility,best-practices,seo --output=html --output-path=./lighthouse-reports/local-sv.html --chrome-flags=\"--headless\" --quiet",
    "lighthouse:prod": "npm run lighthouse:prod:en && npm run lighthouse:prod:sv",
    "lighthouse:prod:en": "lighthouse https://alexandrahedin.se --only-categories=performance,accessibility,best-practices,seo --output=html --output-path=./lighthouse-reports/prod-en.html --chrome-flags=\"--headless\" --quiet",
    "lighthouse:prod:sv": "lighthouse https://alexandrahedin.se/sv/ --only-categories=performance,accessibility,best-practices,seo --output=html --output-path=./lighthouse-reports/prod-sv.html --chrome-flags=\"--headless\" --quiet",
    "lighthouse": "npm run lighthouse:local"
  },
  "dependencies": {
    "astro": "^5.0.5",
    "photoswipe": "^5.3.8",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "lighthouse": "^12.2.1",
    "prettier": "^2.8.8",
    "prettier-plugin-astro": "^0.10.0",
    "tailwindcss": "^4.1.18"
  }
}
