---
import photoImage from '../../assets/aaIMG_1188.jpg';
import ContentSection from '../ContentSection.astro';
import ImageGallery from '../ImageGallery.astro';

export interface Props {
    title: string;
    galleryTitle: string;
    skipLinkText?: string;
}

const { title, galleryTitle, skipLinkText } = Astro.props;
---

<ContentSection
    headerImage={photoImage}
    {title}
    sectionType='photo'
    skipToSection='contact'
    {skipLinkText}
>
    <ImageGallery title={galleryTitle} />
</ContentSection>